/* miniprogram/pages/more/mine/modification.wxss */
.container {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
}
.bgview{
    background-color: #404040; /* 不支持线性的时候显示 */
}

.bg-go{
    background: linear-gradient(to bottom, #16222A, #3A6073);
}
.title {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
}

.note {
    background: rgba(255, 255, 255, 0.5);
    border-radius: 20rpx;
}

.pub_icon {
    width: 50rpx;
    height: 50rpx;
}
